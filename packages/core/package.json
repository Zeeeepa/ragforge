{
  "name": "@luciformresearch/ragforge",
  "version": "0.3.3",
  "description": "Unified RagForge library - YAML config parsing, Neo4j schema introspection, TypeScript code generation, LLM execution, agents, and RAG runtime.",
  "license": "LRSL-1.1 (See LICENSE file for more information)",
  "author": "Lucie Defraiteur <luciedefraiteur@luciformresearch.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuciformResearch/ragforge.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/LuciformResearch/ragforge/issues"
  },
  "homepage": "https://github.com/LuciformResearch/ragforge#readme",
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "copy:templates": "cp -r templates dist/",
    "copy:defaults": "cp -r src/defaults dist/",
    "build": "npm run clean && npm run build:esm && npm run build:types && npm run copy:templates && npm run copy:defaults",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "format": "prettier -w ."
  },
  "keywords": [
    "rag",
    "neo4j",
    "code-generation",
    "typescript",
    "yaml",
    "schema",
    "meta-framework",
    "compiler",
    "runtime",
    "query-builder",
    "vector-search",
    "embeddings",
    "agents",
    "llm"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.28.0",
    "@google/generative-ai": "^0.24.1",
    "@luciformresearch/codeparsers": "file:../../../packages/codeparsers",
    "@luciformresearch/xmlparser": "^0.2.4",
    "@types/adm-zip": "^0.5.7",
    "@types/three": "^0.181.0",
    "@vscode/ripgrep": "^1.17.0",
    "adm-zip": "^0.5.16",
    "chokidar": "^3.5.3",
    "diff": "^8.0.2",
    "dotenv": "^16.3.1",
    "fast-glob": "^3.3.2",
    "fast-xml-parser": "^4.3.0",
    "fastest-levenshtein": "^1.0.16",
    "glob-to-regexp": "^0.4.1",
    "js-yaml": "^4.1.1",
    "mammoth": "^1.11.0",
    "neo4j-driver": "^5.14.0",
    "p-limit": "^4.0.0",
    "pdf-to-img": "^5.0.0",
    "pdf2json": "^4.0.0",
    "playwright": "^1.57.0",
    "replicate": "^1.4.0",
    "strip-json-comments": "^5.0.3",
    "tesseract.js": "^6.0.1",
    "three": "^0.181.2",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
